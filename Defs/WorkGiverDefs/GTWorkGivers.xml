<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ============= Fabricating ============= -->
	<WorkGiverDef Name="GTWorkGiver" Abstract="True">
		<giverClass>WorkGiver_DoBill</giverClass>
		<workType>Crafting</workType>
		<priorityInType>90</priorityInType>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>
	

	<WorkGiverDef ParentName="GTWorkGiver">
		<defName>DoBillsMatterfab</defName>
