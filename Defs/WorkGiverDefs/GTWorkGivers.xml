		<fixedBillGiverDefs>
			<li>Matterfab</li>
		</fixedBillGiverDefs>
		<verb>fabricate</verb>
		<gerund>fabricating</gerund>
	</WorkGiverDef>

	<WorkGiverDef ParentName="GTWorkGiver">
