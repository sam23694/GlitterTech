		<fixedBillGiverDefs>
			<li>RobAssem</li>
		</fixedBillGiverDefs>
		<verb>assemble</verb>
		<gerund>assembling</gerund>
	</WorkGiverDef>

	<WorkGiverDef ParentName="GTWorkGiver">
		<defName>DoBillsPharma</defName>
